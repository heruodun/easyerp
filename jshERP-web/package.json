  },
  "dependencies": {
    "@antv/data-set": "^0.11.2",
    "@tinymce/tinymce-vue": "^2.0.0",
    "ant-design-vue": "^4.2.2",
    "area-data": "^5.0.6",
    "axios": "^0.18.0",
    "clipboard": "^2.0.4",
    "codemirror": "^5.46.0",
    "concurrent-tasks": "^1.0.7",
    "dayjs": "^1.8.0",
    "enquire.js": "^2.1.6",
    "intro.js": "^4.2.2",
    "jquery": "^1.12.4",
    "js-cookie": "^2.2.0",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "md5": "^2.2.1",
    "moment": "^2.30.1",
    "nprogress": "^0.2.0",
    "v-viewer": "^3.0.21",
    "viewerjs": "^1.11.7",
    "vue": "^3.5.16",
    "vue-area-linkage": "^5.1.0",
    "vue-cropper": "^0.4.8",
    "vue-draggable-resizable": "^2.3.0",
    "vue-i18n": "^9.9.0",
    "vue-loader": "^17.4.0",
    "vue-ls": "^4.2.0",
    "vue-plugin-hiprint": "^0.0.60",
    "vue-print-nb-jeecg": "^1.0.9",
    "vue-router": "^4.3.0",
    "vue-splitpane": "^1.0.4",
    "vue3-photo-preview": "^0.3.0",
